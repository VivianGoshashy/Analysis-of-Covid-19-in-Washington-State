# SIR MODEL ODE'S

DE1 = D[s[t], t] == -\[Alpha]/N*s[t]*i[t];
DE2 = D[i[t], t] == \[Alpha]/N*s[t]*i[t] - \[Lambda]*i[t];
DE3 = D[r[t], t] == \[Lambda]* i[t];
Prep = {DE1, DE2, DE3};

# Parameters

\[Alpha] = 1/2;
\[Lambda] = 1/3;

# Plotting SIR Model

initial = {s[0] == 1, i[0] == 1.27*10^(-6), r[0] == 0};
solution = 
  Flatten[NDSolve[Join[Prep, initial], {s, i, r}, {t, 0, 150}]];
splot = Plot[Evaluate[s[t] /. solution[[1]]], {t, 0, 150}, 
   PlotStyle -> RGBColor[0, 0, 1], DisplayFunction -> Identity, 
   PlotLegends -> {"Susceptible Individuals"}];
iplot = Plot[Evaluate[i[t] /. solution[[2]]], {t, 0, 150}, 
   PlotStyle -> RGBColor[1, 0, 0], DisplayFunction -> Identity, 
   PlotLegends -> {"Infected Individuals"}];
rplot = Plot[Evaluate[r[t] /. solution[[3]]], {t, 0, 150}, 
   PlotStyle -> RGBColor[0, 1, 0], DisplayFunction -> Identity, 
   PlotLegends -> {"Removed Individuals"}];
Show[splot, iplot, rplot, AxesLabel -> {"t", "s,i,r"}, 
 PlotRange -> {0, 1}, DisplayFunction -> $DisplayFunction, 
 PlotLabel -> "COVID-19 SIR Model"]
 
 # Fitting Infected people with COVID-19 Data to the ODE'S and Estimating Parameters (Alpha and Lambda) 
 
 mymodel[alpha_?NumberQ, lambda_?NumberQ] := 
 Module[{S, I1, R1, t}, 
  First[I1 /. 
    NDSolve[{S'[t] == -alpha*S[t]*I1[t], 
      I1'[t] == alpha*S[t]*I1[t] - lambda*I1[t], 
      R1'[t] == lambda*I1[t], S[0] == 300000, I1[0] == 10000, 
      R1[0] == 0}, {S, I1, R1}, {t, 0, 420}]]]
myfit = NonlinearModelFit[Cov2, 
   mymodel[alpha, lambda][
    t], {{alpha, 0.000005}, {lambda, 0.0000001}}, t];
myfit["ParameterTable"]
plotfit1 = 
  Plot[myfit[t], {t, 0, 420}, PlotRange -> All, AxesOrigin -> {0, 0}, 
   PlotLegends -> {"Fitted Model"}, PlotStyle -> Red, 
   AxesLabel -> {"Time(t)", "Infected People (I)"}, 
   PlotLabel -> "Covid-19 Fitted Model of Infected Individuals"];
Show[plotfit1, plot1]

# Fitting Infected people with COVID-19 Data to the ODE'S and Estimating Parameters (alpha, beta, gamma, lambda, phi)

mymodel[alpha_?NumberQ, lambda_?NumberQ, gamma_?NumberQ, 
  beta_?NumberQ, phi_?NumberQ] := 
 Module[{S, I1, R1, t}, 
  First[I1 /. 
    NDSolve[{S'[t] == -alpha*S[t]*I1[t], 
      I1'[t] == alpha*S[t]*I1[t] - lambda*I1[t], 
      R1'[t] == lambda*I1[t], S[0] == gamma, I1[0] == beta, 
      R1[0] == phi}, {S, I1, R1}, {t, 0, 420}]]]
myfit1 = NonlinearModelFit[Cov2, 
   mymodel[alpha, lambda][
    t], {{alpha, 0.000005}, {lambda, 0.0000001}, {gamma, 
     300000}, {beta, 10000}, {phi, 0}}, t];
myfit1["ParameterTable"]
plotfit2 = 
 Plot[myfit1[t], {t, 0, 420}, PlotRange -> All, AxesOrigin -> {0, 0}, 
  PlotLegends -> {"Fitted Model"}, PlotStyle -> Red, 
  AxesLabel -> {"Time(t)", "Infected People (I)"}, 
  PlotLabel -> 
   "Covid-19 Fitted Model of Infected Individuals"]; Show[plotfit2, \
plot1]

# Obtain the residuals and plot them for each data point

ListPlot[myfit1["FitResiduals"]]

# Generate a plot of standardized residuals for the fitted Nonlinear model

ListPlot[myfit1["StandardizedResiduals"], Frame -> True, 
 Filling -> Axis]
 
