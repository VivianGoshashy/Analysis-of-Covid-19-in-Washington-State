# Importing Washington State Covid-19 Dataset

Cov = Import[
  "C:\\Users\\vivia\\Documents\\MS in Applied Mathematics\\Fall \
2021\\Non Linear Dynamics\\Project\\Data\\WA-COVID-19.csv", "Dataset",
   "HeaderLines" -> 1]
   
# Selecting number of days and number of infected people columns in the original dataset

Cov2 = Cov [All, {"Number of days", "Number of Infected People"}];

# Converting Dataset to List

time = Normal[Cov2[[All, "Number of days"]]];
People = Normal[Cov2[[All, "Number of Infected People"]]];
Cov2 = Transpose[{time, People}];

# Plotting a new dataset that has only infected people

plot1 = ListLinePlot[Cov2, PlotStyle -> {Black, PointSize[0.01]}, 
  PlotLegends -> {"Infected people dataset"}, PlotStyle -> Black, 
  AxesLabel -> {"Time(t)", "Infected People (I)"}, 
  PlotLabel -> "Number of Covid-19 infected people in WA"]
  
# Selecting number of days and number of removed people columns in the original dataset

Cov1 = Cov [All, {"Number of days", "Number of deaths"}];

# Converting Dataset to List

time = Normal[Cov1[[All, "Number of days"]]];
deaths = Normal[Cov1[[All, "Number of deaths"]]];
Cov1 = Transpose[{time, deaths}];

# Plotting a new dataset that has only removed people

plot2 = ListLinePlot[Cov1, PlotStyle -> {Red, PointSize[0.01]}, 
  PlotLegends -> {"Removed people dataset"}, PlotStyle -> Black, 
  AxesLabel -> {"Time(t)", "Removed People (R)"}, 
  PlotLabel -> "Number of Removed Individuals from COVID-19 in WA"]
  
  
# Selecting number of days and number of susceptible people columns in the original dataset

Cov3 = Cov [All, {"Number of days", "Total Population"}];

# Converting Dataset to List

time = Normal[Cov3[[All, "Number of days"]]];
Susceptible = Normal[Cov3[[All, "Total Population"]]];
Cov3 = Transpose[{time, Susceptible}];

# Plotting a new dataset that has only susceptible people

plot3 = ListLinePlot[Cov3, PlotStyle -> {Green, PointSize[0.01]}, 
  PlotLegends -> {"Susceptible"}, PlotStyle -> Black, 
  AxesLabel -> {"Time(t)", "Susceptible People (S)"}, 
  PlotLabel -> 
   "Number of Susceptible Individuals from COVID-19 in WA", 
  PlotRange -> All]
  
# Selecting Number of days , Number of Infected People, and Number of dead People Columns 

Cov4 = Cov [
   All, {"Number of days", "Number of Infected People", 
    "Number of deaths", "Total Population"}];
    
# Converting Dataset to List

time = Normal[Cov4[[All, "Number of days"]]];
People = Normal[Cov4[[All, "Number of Infected People"]]];
deaths = Normal[Cov4[[All, "Number of deaths"]]];
Susceptible = Normal[Cov4[[All, "Total Population"]]];
Cov4 = Transpose[{time, People, deaths}];

# Plot of Susceptible, Infected, and Removed group

Show[plot1, plot2, plot3]

